<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    p1:gravity="center_vertical"
    p1:id="@+id/linearLayout1"
    p1:layout_height="match_parent"
    p1:layout_width="match_parent"
    p1:orientation="vertical"
    p1:paddingBottom="@dimen/activity_vertical_margin"
    p1:paddingLeft="@dimen/activity_horizontal_margin"
    p1:paddingRight="@dimen/activity_horizontal_margin"
    p1:paddingTop="@dimen/activity_vertical_margin">
    <TextView
        p1:gravity="center_horizontal"
        p1:id="@+id/textView1"
        p1:layout_height="wrap_content"
        p1:layout_width="match_parent"
        p1:paddingBottom="8dp"
        p1:text="Xamarin.Android Performance"
        p1:textAppearance="?android:attr/textAppearanceMedium" />
    <TextView
        p1:gravity="center_horizontal"
        p1:id="@+id/textViewArithmetic"
        p1:layout_height="wrap_content"
        p1:layout_width="match_parent"
        p1:paddingBottom="8dp"
        p1:text="Arithmetic: "
        p1:textAppearance="?android:attr/textAppearanceMedium" />
    <TextView
        p1:gravity="center_horizontal"
        p1:id="@+id/textViewCollections"
        p1:layout_height="wrap_content"
        p1:layout_width="match_parent"
        p1:paddingBottom="8dp"
        p1:text="Collections: "
        p1:textAppearance="?android:attr/textAppearanceMedium" />
    <TextView
        p1:gravity="center_horizontal"
        p1:id="@+id/textViewStrings"
        p1:layout_height="wrap_content"
        p1:layout_width="match_parent"
        p1:paddingBottom="16dp"
        p1:text="Strings: "
        p1:textAppearance="?android:attr/textAppearanceMedium" />
    <Button
        p1:id="@+id/buttonCalculate"
        p1:layout_height="wrap_content"
        p1:layout_width="match_parent"
        p1:text="Calculate" />
</LinearLayout>